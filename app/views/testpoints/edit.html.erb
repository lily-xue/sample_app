 
<% provide(:title, "Edit testpoint") %>
<%= form_for (@testpoint) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

<%= f.label :测试点 %>
<%= f.text_area :point, placeholder: "填写测试目的..." %>
<%= f.label :测试步骤 %>
<%= f.text_area :step %>


  <%= f.submit "发布", class: "btn btn-large btn-primary" %>
<% end %>

